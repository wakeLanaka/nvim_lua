{
  "textit" : {
    "prefix": ["it"],
    "body": [
      "\\textit{$1} $0"
    ]
  },
  "input" : {
    "prefix": ["input"],
    "body": [
      "\\input{$0}"
    ]
  },
  "itemize" : {
    "prefix": ["itemi"],
    "body": [
      "\\begin{itemize}",
      "\t\\item $0",
      "\\end{itemize}"
    ]
  },
  "enumerate" : {
    "prefix": ["enum"],
    "body": [
      "\\begin{enumerate}",
      "\t\\item $0",
      "\\end{enumerate}"
    ]
  },
  "section" : {
    "prefix": ["sect", "\\sect"],
    "body": [
      "\\section{$1}",
      "$0"
    ]
  },
  "subsection" : {
    "prefix": ["subsect"],
    "body": [
      "\\subsection{$1}",
      "$0"
    ]
  },
  "subsubsection" : {
    "prefix": ["subsubsect"],
    "body": [
      "\\subsubsection{$1}",
      "$0"
    ]
  },
  "paragraph" : {
    "prefix": ["paragraph"],
    "body": [
      "\\paragraph{$1}",
      "$0"
    ]
  },
  "textcolorRED" : {
    "prefix": ["tcr"],
    "body": [
      "\\textcolor{red}{$1} $0"
    ]
  },
  "bold" : {
    "prefix": ["bf"],
    "body": [
      "\\textbf{$1} $0"
    ]
  },
  "rightarrow" : {
    "prefix": ["rightarrow"],
    "body": [
      "\\$\\rightarrow\\$ $0"
    ]
  },
  "includegraphics" : {
    "prefix": ["includegraphics"],
    "body": [
      "\\includegraphics[width=(0.${1:24}\\textwidth)]{$2}$0"
    ]
  }
}
